<div class="index_container">
    <h1 class="users-heading">ユーザー一覧</h1>
    <!-- 以下の<% %>を使ってeach文を追加してください -->
    <% @user.each do |post| %>
      <div class="users-index-item">
        <div class="user-left">
          <!-- <img>タグを用いて、プロフィール画像を表示してください-->
          <img src="<%="/user_images/#{post.image_name}"%>">
        </div>
        <div class="user-right">
          <%= link_to(post.name, "/users/#{post.id}") %>
        </div>
      </div>
    <% end %>
</div>